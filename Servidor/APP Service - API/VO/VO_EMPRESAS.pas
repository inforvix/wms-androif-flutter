Function TFin_empresasVO.PerfilArqSpedFiscalToString(PerfilArqSpedFiscal: TPerfilArqSpedFiscal): String;
Begin
  Case PerfilArqSpedFiscal Of
    mpfPerfilA: result := 'A';
    mpfPerfilB: result := 'B';
    mpfPerfilC: result := 'C';
  Else
    Begin
      Raise Exception.Create('Não Implementado !');
    End;
  End;
End;

function TFin_empresasVO.TipoAtividadeToInteger(TipoAtividade: TIndAtivSpedFiscal): Integer;
begin
  Case TipoAtividade Of
    matIndustrial: result := 0;
    matOutros: result := 1;
  Else
    Begin
      Raise Exception.Create('Não Implementado !');
    End;
  End;
end;

function TFin_empresasVO.IntegerToArqTipoAtividade(IntTipoAtividade: Integer): TIndAtivSpedFiscal;
begin
  case IntTipoAtividade of
    0 : Result := matIndustrial;
    1 : Result := matOutros;
  end;
end;

Function TFin_empresasVO.StringToPerfilArqSpedFiscal(StrPerfilArqSpedFiscal: String): TPerfilArqSpedFiscal;
Begin
  if StrPerfilArqSpedFiscal = '' then
    Exit;

  If StrPerfilArqSpedFiscal = 'A' Then
    result := mpfPerfilA
  Else If StrPerfilArqSpedFiscal = 'B' Then
    result := mpfPerfilB
  Else If StrPerfilArqSpedFiscal = 'C' Then
    result := mpfPerfilC
  Else
  Begin
    Raise Exception.Create('Não Implementado !');
  End;
End;

